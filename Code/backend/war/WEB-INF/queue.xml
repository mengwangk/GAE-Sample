<!-- See Queues.java for docs -->
<queue-entries>
	<total-storage-limit>500M</total-storage-limit>	<!-- default is 100M -->
	
	<!-- This is as fast as we can make them -->
	
	<queue>
		<name>default</name>
		<rate>500/s</rate>
		<bucket-size>100</bucket-size>
		<max-concurrent-requests>100</max-concurrent-requests>
	</queue>
	
	<queue>
		<name>sync</name>
		<rate>500/s</rate>
		<bucket-size>100</bucket-size>
		<max-concurrent-requests>100</max-concurrent-requests>
	</queue>
</queue-entries>
